{
  "name": "diabetic-utils",
  "version": "1.1.6",
  "description": "Zero-bloat TypeScript utilities for diabetes data: glucose, A1C, conversions, time-in-range, and more.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marklearst/diabetic-utils.git"
  },
  "homepage": "https://github.com/marklearst/diabetic-utils#readme",
  "bugs": {
    "url": "https://github.com/marklearst/diabetic-utils/issues"
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch",
    "build": "tsup",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist",
    "docs:api": "typedoc"
  },
  "keywords": [
    "diabetes",
    "glucose",
    "a1c",
    "health",
    "typescript",
    "utilities",
    "mgdl",
    "mmol",
    "time-in-range",
    "medical"
  ],
  "author": "Mark Learst",
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "@types/node": "^22.15.3",
    "@vitest/coverage-v8": "^3.1.2",
    "@vitest/ui": "^3.1.2",
    "tsup": "^8.4.0",
    "typedoc": "0.28.4",
    "typedoc-plugin-markdown": "^4.6.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  },
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  }
}
